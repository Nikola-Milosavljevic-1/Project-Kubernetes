{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "Vue": "readonly"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "linebreak-style": "off",
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "never"],
    "no-use-before-define": "off"
  }
}
